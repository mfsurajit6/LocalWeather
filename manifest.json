{
    "manifest_version": 2,
    "name": "Local Weather",
    "description": "Get weather information of your locality",
    "version": "1.0",
    "icons": {"128": "images/icon_128.png"},
    "browser_action": {
        "default_icon": "images/icon_19.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "geolocation"]
}